#include <stdio.h>
#include <locale.h>
//Progrma que hace una lista de numero positivos y si sale uno negativo termina
int main() {
    setlocale(LC_ALL, "spanish");
	
    int num;
    
    printf("Proporciona números para hacer una lista de números positivos:\n\n");
    scanf("%d", &num);
    
    while (num>0){
    	
    	scanf("%d", &num);
    	
    	if (num<0){
    		break;
    	}
	}
    return 0;
}
